@section page_title{
Analysis
}

@using Web.Areas.AnalysisManagement.Controllers;
@using Web.Areas.AnalysisManagement;
@using Microsoft.Web.Mvc;


@{
    Html.RenderPartial("_EmptySubNavigation");
    Html.RenderPartial("_EmptySideBar");
}
<script type="text/javascript">

    Ext.onReady(function () {

        var analysisBtnId = window.res.header.navigation.analysisAndReports;
        var analysisBtn = Ext.getCmp(analysisBtnId);
        analysisBtn.toggle(true);

    });

</script>
<script type="text/javascript">
    Ext.onReady(function () {

        window.feature = {};

        window.feature.container = Ext.create('Ext.container.Container', {
            margin: '15 25 15 15',
            padding: '20 20 20 20',
            id: 'TopContainer',
            layout: {
                align: 'stretch',
                type: 'hbox'
            },
            autoScroll: true,
            items: [{
                xtype: 'container',
                width: 1200,
                height: 500,
                autoScroll: true,
                layout: {
                    align: 'stretch',
                    type: 'vbox'
                },
                items: [{
                    xtype: 'container',
                    flex: 1,
                    layout: {
                        align: 'stretch',
                        type: 'hbox'
                    },
                    items: [{
                        xtype: 'container',
                        flex: 1,
                        layout: {
                            type: 'table',
                            columns: 2
                        },
                        items: [{
                            xtype: 'image',
                            src: '@(Url.RouteUrl<Web.Controllers.AssetsController>(it => it.Shared("/img/Analysis/OutpostList.png"), Web.Bootstrap.Routes.AssetRoutesRegistrar.SHARED))',
                            margin: '20 20 0 20',
                            rowspan: 2
                        }, {
                            xtype: 'box',
                            autoEl: { tag: 'a', cls: 'blue-label', href: '@(Url.RouteUrl<PatientsReportController>(x=>x.Overview(),AnalysisManagementAreaRegistration.DEFAULT_ROUTE))', html: 'Patient Report' },
                            margin: '70 30 10 0'
                        }, {
                            xtype: 'label',
                            margin: '0 0 10 0',
                            text: 'In this report the user can see all information on patients.'
                        }]
                    }, {
                        xtype: 'container',
                        flex: 1,
                        layout: {
                            type: 'table',
                            columns: 2
                        },
                        items: [{
                            xtype: 'image',
                            src: '@(Url.RouteUrl<Web.Controllers.AssetsController>(it => it.Shared("/img/Analysis/ActivityReport.png"), Web.Bootstrap.Routes.AssetRoutesRegistrar.SHARED))',
                            margin: '20 20 0 20',
                            rowspan: 2
                        }, {
                            xtype: 'box',
                            autoEl: { tag: 'a', cls: 'blue-label', href: '@(Url.RouteUrl<HealthFacilityReportController>(x=>x.Overview(),AnalysisManagementAreaRegistration.DEFAULT_ROUTE))', html: 'Health Facility Report' },
                            margin: '70 30 10 0'
                        }, {
                            xtype: 'label',
                            margin: '0 0 10 0',
                            text: 'This report will show the number of patients received at Health Facilities with gender stratification.'
                        }]
                    }]
                }, {
                    xtype: 'container',
                    flex: 1,
                    layout: {
                        align: 'stretch',
                        type: 'hbox'
                    },
                    items: [{
                        xtype: 'container',
                        flex: 1,
                        layout: {
                            type: 'table',
                            columns: 2
                        },
                        items: [{
                            xtype: 'image',
                            src: '@(Url.RouteUrl<Web.Controllers.AssetsController>(it => it.Shared("/img/Analysis/ActivityReport.png"), Web.Bootstrap.Routes.AssetRoutesRegistrar.SHARED))',
                            margin: '20 20 0 20',
                            rowspan: 2
                        }, {
                            xtype: 'box',
                            autoEl: { tag: 'a', cls: 'blue-label', href: '@(Url.RouteUrl<DiagnosisReportController>(x=>x.Overview(),AnalysisManagementAreaRegistration.DEFAULT_ROUTE))', html: 'Diagnosis Report' },
                            margin: '70 30 10 0'
                        }, {
                            xtype: 'label',
                            margin: '0 0 10 0',
                            text: 'This report will show the number of patients who received a specific diagnosis at different health facilities.'
                        }]
                    }, {
                        xtype: 'container',
                        flex: 1,
                        layout: {
                            type: 'table',
                            columns: 2
                        },
                        items: [{
                            xtype: 'image',
                            src: '@(Url.RouteUrl<Web.Controllers.AssetsController>(it => it.Shared("/img/Analysis/ActivityReport.png"), Web.Bootstrap.Routes.AssetRoutesRegistrar.SHARED))',
                            margin: '20 20 0 20',
                            rowspan: 2
                        }, {
                            xtype: 'box',
                            autoEl: { tag: 'a', cls: 'blue-label', href: '@(Url.RouteUrl<TreatmentReportController>(x=>x.Overview(),AnalysisManagementAreaRegistration.DEFAULT_ROUTE))', html: 'Treatments Report' },
                            margin: '70 30 10 0'
                        }, {
                            xtype: 'label',
                            margin: '0 0 10 0',
                            text: 'This report will show the number of patients who received a specific treatment at different health facilities.'
                        }]
                    }]
                }, {
                    xtype: 'container',
                    flex: 1,
                    layout: {
                        align: 'stretch',
                        type: 'hbox'
                    },
                    items: [{
                        xtype: 'container',
                        flex: 1,
                        layout: {
                            type: 'table',
                            columns: 2
                        },
                        items: [{
                            xtype: 'image',
                            src: '@(Url.RouteUrl<Web.Controllers.AssetsController>(it => it.Shared("/img/Analysis/ActivityReport.png"), Web.Bootstrap.Routes.AssetRoutesRegistrar.SHARED))',
                            margin: '20 20 0 20',
                            rowspan: 2
                        }, {
                            xtype: 'box',
                            autoEl: { tag: 'a', cls: 'blue-label', href: '@(Url.RouteUrl<AdviceReportController>(x=>x.Overview(),AnalysisManagementAreaRegistration.DEFAULT_ROUTE))', html: 'Advice Report' },
                            margin: '70 30 10 0'
                        }, {
                            xtype: 'label',
                            margin: '0 0 10 0',
                            text: 'This report will show the number of patients who received a specific advice at different health facilities.'
                        }]
                    }, {
                        xtype: 'container',
                        flex: 1,
                        layout: {
                            type: 'table',
                            columns: 2
                        },
                        items: [{
                            xtype: 'image',
                            src: '@(Url.RouteUrl<Web.Controllers.AssetsController>(it => it.Shared("/img/Analysis/ActivityReport.png"), Web.Bootstrap.Routes.AssetRoutesRegistrar.SHARED))',
                            margin: '20 20 0 20',
                            rowspan: 2
                        }, {
                            xtype: 'box',
                            autoEl: { tag: 'a', cls: 'blue-label', href: '@(Url.RouteUrl<SummaryReportController>(x=>x.Overview(),AnalysisManagementAreaRegistration.DEFAULT_ROUTE))', html: 'Summary Report' },
                            margin: '70 30 10 0'
                        }, {
                            xtype: 'label',
                            margin: '0 0 10 0',
                            text: 'This report will show the number of patients who failed to report at a certain dispensary, number of patients who obtained/ did not get treatment and the number of patients reported into the system at different dispensaries or health centers.'
                        }]
                    }]
                }, {
                    xtype: 'container',
                    flex: 1,
                    layout: {
                        align: 'stretch',
                        type: 'hbox'
                    },
                    items: [{
                        xtype: 'container',
                        flex: 1,
                        layout: {
                            type: 'table',
                            columns: 2
                        },
                        items: [{
                            xtype: 'image',
                            src: '@(Url.RouteUrl<Web.Controllers.AssetsController>(it => it.Shared("/img/Analysis/StockLevelRegion.png"), Web.Bootstrap.Routes.AssetRoutesRegistrar.SHARED))',
                            margin: '20 20 0 20',
                            rowspan: 2
                        }, {
                            xtype: 'box',
                            autoEl: { tag: 'a', cls: 'blue-label', href: '@(Url.RouteUrl<GoogleMapReportController>(x=>x.Overview(),AnalysisManagementAreaRegistration.DEFAULT_ROUTE))', html: 'Google Map Report' },
                            margin: '70 30 10 0'
                        }, {
                            xtype: 'label',
                            margin: '0 0 10 0',
                            text: 'This report will show the number of patients referred and treated at different health facilities.'
                        }]
                    }, {
                        xtype: 'container',
                        flex: 1

                    }]

                }]
            }]
        });

        var contentRes = window.res.content;
        var mainContent = Ext.getCmp(contentRes.maincontent.id);

        mainContent.add(window.feature.container);
    });
</script>
